q1 = int(input("quantidade:"))
cd1 = int(input("valor:")) 
print (cd1*q1)
q2 = int(input("quantidade:"))
cd2 = int(input("valor:"))
print (cd2*q2)
q3 = int(input("quantidade:"))
cd3 = int(input("valor:"))
print(cd3*q3)
q4 = int(input("quantidade:"))
cd4 = int(input("valor:"))
print (cd4*q4)
q5 = int(input("quantidade:"))
cd5 = int(input("valor:"))
print (cd5*q5)
print ('o investimento é de:',cd1*q1+cd2*q2+cd3*q3+cd4*q4+cd5*q5)
print ('a média é de:',(cd1*q1+cd2*q2+cd3*q3+cd4*q4+cd5*q5)/q1+q2+q3+q4+q5)